<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.huawei.android.launcher">
    <!--第 0 屏-->
    <!--第 1 屏-->
    <appwidget
        launcher:packageName="com.huawei.android.totemweather"
        launcher:className="com.huawei.android.totemweather.widget.doublecity.DualWidgetWeatherProvider"
        launcher:op="delete"/>
    <appwidget
        launcher:packageName="com.android.gallery3d"
        launcher:className="com.android.gallery3d.gadget.PhotoAppWidgetProvider"
        launcher:op="delete" />
    <appwidget
        launcher:packageName="com.android.mediacenter"
        launcher:className="com.android.mediacenter.localmusic.MediaAppWidgetProvider"
        launcher:op="delete" />
    <!-- 天气时钟 -->
    <appwidget
        launcher:packageName="com.huawei.android.totemweather"
        launcher:className="com.huawei.android.totemweather.widget.mulan.MulanWidgetWeatherProvider"
        launcher:container="-100"
        launcher:screen="1"
        launcher:x="0"
        launcher:y="0"
        launcher:spanX="4"
        launcher:spanY="2"
        launcher:op="insert"/>
    <!--第 2 屏-->
    <!-- 运动健康，海外-->
    <favorite
        launcher:packageName="com.huawei.health"
        launcher:className="com.huawei.healthcloud.cardui.MainActivity"
        launcher:container="-100"
        launcher:screen="2"
        launcher:x="0"
        launcher:y="2"
        launcher:op="insertinto"/>
    <!-- Top APPS-->
    <folder
        launcher:title="@string/folder_top_apps"
        launcher:container="-100"
        launcher:screen="2"
        launcher:x="3"
        launcher:y="2">
        <!-- facebook -->
        <favorite
            launcher:packageName="com.facebook.katana"
            launcher:className="com.facebook.katana..LoginActivity"/>
        <!-- Twitter -->
        <favorite
            launcher:packageName="com.twitter.android"
            launcher:className="com.twitter.android.StartActivity"/>
        <!-- News Republic -->
        <favorite
            launcher:packageName="com.mobilesrepublic.appy"
            launcher:className="com.mobilesrepublic.appy.SplashActivity"/>
        <!-- booking -->
        <favorite
            launcher:packageName="com.booking"
            launcher:className="com.booking.activity.StartActivity"/>
        <!-- Lyft -->
        <favorite
            launcher:packageName="me.lyft.android"
            launcher:className="me.lyft.android.ui.MainActivity"/>
        <!-- TripAdvisor -->
        <favorite
            launcher:packageName="com.tripadvisor.tripadvisor"
            launcher:className="com.tripadvisor.tripadvisor.TripAdvisorTripAdvisorActivity"/>
        <!-- CNN -->
        <favorite
            launcher:packageName="com.cnn.mobile.android.phone"
            launcher:className="com.cnn.mobile.android.phone.ui.SplashActivity"/>
    </folder>
</favorites>
